public OnPlayerClickMap(playerid, Float:fX, Float:fY, Float:fZ){
	new Float:x,Float:y,Float:z;
	/*,Float:maz;*/
	GetPlayerPos(playerid,x,y,z);
	
	MapClick[playerid][T3D:X] = fX;
	MapClick[playerid][T3D:Y] = fY;
	
	//Tryg3DMapAndreasFindZ(MapClick[playerid][T3D:X],MapClick[playerid][T3D:Y],maz);
	//SetDynamicRaceCPPos(PlayerMarker[playerid],MapClick[playerid][T3D:X],MapClick[playerid][T3D:Y],maz);
	
	if(GetDistanceBetweenPoints2D(fX,fY,x,y) > 25.0){
		SetDynamicCirclePos(PlayerTTMArea[playerid],fX,fY);
		PlayerTTMTick[playerid] = GetTickCount();
	} else {
		SetDynamicCirclePos(PlayerTTMArea[playerid],0.0,-1000.0);
	}

	return 1;
}

