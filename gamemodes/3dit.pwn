forward TimerLicznikSpeed(tec);
public TimerLicznikSpeed(tec){
	FoxForeach(i,Player){
		//if(!CG_ProtVar::cgod_clear[i]) continue;
		new lstr[1000], string[200];
		if(gGracz[i][towar] > 0){
			if(gGracz[i][TrybFilmowy] == false){
				format(string,sizeof(string),"%s [%i t.]",TowarNazwa[gGracz[i][towar]],gGracz[i][TowarTony]);
				PlayerTextDrawSetString(i,PasekStanu5[i],string);
				TextDrawShowForPlayer(i,TruckTD);
				PlayerTextDrawShow(i,PasekStanu5[i]);
			}
		} else {
			PlayerTextDrawHide(i,PasekStanu5[i]);
			TextDrawHideForPlayer(i,TruckTD);
		}
		
		if(IsPlayerInAnyVehicle(i)){
			new vid = GetPlayerVehicleID(i), miD = GetVehicleModel(vid);
			if(miD >= 400 && miD <= 611){
				if(gGracz[i][TrybFilmowy] == false){
					if(miD == 481 || miD == 509 || miD == 510){
						PlayerTextDrawShow(i,VehicleName[i]);
						PlayerTextDrawShow(i,LicznikSpeed[i]);
						format(lstr, sizeof(lstr), "Predkosc: %.0f km/h",GetVehicleSpeed(vid));
						PlayerTextDrawSetString(i,LicznikSpeed[i], lstr);
						format(lstr, sizeof(lstr), "%s",VehicleNames[miD-400]);
						PlayerTextDrawSetString(i,VehicleName[i],lstr);
						TextDrawShowForPlayer(i,Sprite3);
					} else {
						new Float:pvpos[3];
						GetVehiclePos(vid, pvpos[0], pvpos[1], pvpos[2]);
						Przebieg[vid][3] += (floatsqroot(floatpower(pvpos[0] - Przebieg[vid][0], 2) + floatpower(pvpos[1] - Przebieg[vid][1], 2) + floatpower(pvpos[2] - Przebieg[vid][2], 2)))/1000;
						Przebieg[vid][0] = pvpos[0];
						Przebieg[vid][1] = pvpos[1];
						Przebieg[vid][2] = pvpos[2];

						TextDrawShowForPlayer(i,Sprite3);
						PlayerTextDrawShow(i,VehicleName[i]);
						PlayerTextDrawShow(i,LicznikSpeed[i]);
						new Float:vehiclehealth;
						GetVehicleHealth(vid, vehiclehealth);
						vehiclehealth = ((vehiclehealth-250.0)/7.5);
						if(floatround(vehiclehealth) == 1) vehiclehealth += 1.0;
						if(floatround(vehiclehealth) < 0) vehiclehealth = 0.0;
						format(lstr, sizeof(lstr), "Predkosc: %.0f~n~Stan: %i%%~n~Paliwo: %.1fL~n~Przebieg: %i",GetVehicleSpeed(vid), floatround(vehiclehealth),Paliwo[vid]);
						PlayerTextDrawSetString(i,LicznikSpeed[i], lstr);
						format(lstr, sizeof(lstr), "%s",VehicleNames[miD-400]);
						PlayerTextDrawSetString(i,VehicleName[i],lstr);
						if(Paliwo[vid] <= 0)
						{
							new lights, alarm, doors, bonnet, boot, objective;
							SetVehicleParamsEx(vid, 0, lights, alarm, doors, bonnet, boot, objective);
							GameTextForPlayer(i, "~r~Brak Paliwa !", 1000, 6);
						}
						lastx[vid] = pvpos[0];
						lasty[vid] = pvpos[1];
						lastz[vid] = pvpos[2];
					}
				}
			}
		} else {
			PlayerTextDrawHide(i,LicznikSpeed[i]);
			PlayerTextDrawHide(i,VehicleName[i]);
			TextDrawHideForPlayer(i,Sprite3);
		}
	}
	return 1;
}




