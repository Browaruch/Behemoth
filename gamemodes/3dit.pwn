public OnPlayerClickPlayer(playerid, clickedplayerid, source)
{
	new string[256];
	new gSpectateID[MAX_PLAYERS];
	new gSpectateType[MAX_PLAYERS];
	new Float:x, Float:y,Float:z;
	GetPlayerPos(playerid,x,y,z);
	if(gGracz[playerid][AdminLvl] >= 3||IsPlayerAdmin(playerid))
	{
		if(clickedplayerid == playerid)
		{
	 		TogglePlayerSpectating(playerid, 0);
			gSpectateID[playerid] = INVALID_PLAYER_ID;
			gSpectateType[playerid] = ADMIN_SPEC_TYPE_NONE;
			format(string,sizeof(string),"{7342FF}Wy³¹czy³eœ specowanie graczy{7342FF}!");
			SendClientMessage(playerid, COLOR_GREEN, string);
		}
		else
		{
			new privek=0;
			FoxForeach(i,Character)
			{
				if(gGracz[i][Zalogowany] == true && IsPlayerInAnyVehicle(clickedplayerid))
				{
					if(IdVehicle[clickedplayerid] == PrywatneAuto[i])
					{
						privek = i;
					}
				}
			}
			if(gGracz[playerid][Specowanie] == false)
			{
				gGracz[playerid][SpecX] = x;
				gGracz[playerid][SpecY] = y;
				gGracz[playerid][SpecZ] = z;
			}
			gGracz[playerid][Specowanie] = true;
			if(GetPlayerState(clickedplayerid) == PLAYER_STATE_DRIVER)
			{
				TogglePlayerSpectating(playerid, 1);
   	 			PlayerSpectateVehicle(playerid, GetPlayerVehicleID(clickedplayerid));
				gSpectateID[playerid] = GetPlayerVehicleID(clickedplayerid);
	  			gSpectateType[playerid] = ADMIN_SPEC_TYPE_VEHICLE;
			}
			else
			{
				SetPlayerVirtualWorld(playerid,GetPlayerVirtualWorld(clickedplayerid));
				SetPlayerInterior(playerid,GetPlayerInterior(clickedplayerid));
				TogglePlayerSpectating(playerid,1);
				PlayerSpectatePlayer(playerid,clickedplayerid);
				gSpectateID[playerid] = clickedplayerid;
				gSpectateType[playerid] = ADMIN_SPEC_TYPE_PLAYER;
			}
			if(privek == 0){format(string,sizeof(string),"{7342FF}Podgl¹dasz gracza: {CBC9CF}%s [%i]{7342FF}! Ping: %i.",GetPlayerNick(clickedplayerid),clickedplayerid,GetPlayerPing(clickedplayerid));}
			else{format(string,sizeof(string),"{7342FF}Podgl¹dasz gracza: {CBC9CF}%s [%i]{7342FF}! Ping: %i Priv: %s[%i].",GetPlayerNick(clickedplayerid),clickedplayerid,GetPlayerPing(clickedplayerid),GetPlayerNick(privek),privek);}
			PlayerPlaySound(playerid, 1149,0,0,0);
			SendClientMessage(playerid, COLOR_GREEN, string);
			if(gGracz[clickedplayerid][AdminLvl] > gGracz[playerid][AdminLvl] )
			{
				format(string,sizeof(string),"Mniejsza ranga {C4C4C4}%s[%i] {FF0000}specuje ciê.",GetPlayerNick(playerid),playerid);
				SendClientMessage(clickedplayerid, COLOR_RED, string);
			}
			format(string,sizeof(string),"*[Spec]%s > %s",GetPlayerNick(playerid),GetPlayerNick(clickedplayerid));
			WriteLog(string,LOG_FILE_ADMIN);
		}
	}
	else
	{
		SendClientMessage(playerid, COLOR_RED, "{FF0000}[B£¥D]{FFFFFF} Nie jesteœ upowa¿niony do u¿ycia tej funkcji.");
	}
	return 1;
}

