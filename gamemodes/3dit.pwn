				return 0;
			}
		}
	}
	
	new medycy = 0,policjanty=0;
	SchowajPasekStanu(playerid);
	FoxForeach(i,Player){
		if(gGracz[i][FrakcjaMedyk] == 1 && SkinyMedykow(i)) medycy++;
