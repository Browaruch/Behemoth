function PozarTimer(PozarID){
	new string[255];
	Pozar[PozarID][PozHP] += 0.02;
	FoxForeach(i,Player){
		if(Pozar[PozarID][PozHP] > 0){
			new Float:HPg;
			GetPlayerHealth(i,HPg);
			new mid = GetVehicleModel(IdVehicle[i]);
			if(IsPlayerAimingAt(i,Pozar[PozarID][Pozx],Pozar[PozarID][Pozy],Pozar[PozarID][Pozz],10)){
				if(PrzyciskFire[i] == true && IsPlayerInRangeOfPoint(i,10,Pozar[PozarID][Pozx],Pozar[PozarID][Pozy],Pozar[PozarID][Pozz]) && GetPlayerWeapon(i) == 42){
					Pozar[PozarID][PozHP] = Pozar[PozarID][PozHP] - 0.5;
					SetPlayerColor(i,0x6DC5F3FF);
				} else if(PrzyciskFire[i] == true && IsPlayerInRangeOfPoint(i,10,Pozar[PozarID][Pozx],Pozar[PozarID][Pozy],Pozar[PozarID][Pozz]) && (mid == 407 || mid == 601)){
					Pozar[PozarID][PozHP] = Pozar[PozarID][PozHP] - 1.0;
					SetPlayerColor(i,0x6DC5F3FF);
				}
			}
			if(IsPlayerInRangeOfPoint(i,Pozar[PozarID][Zasieg],Pozar[PozarID][Pozx],Pozar[PozarID][Pozy],Pozar[PozarID][Pozz])){
				if(HPg > 0){
					SetPlayerHealth(i,HPg - 0.1);
				}
			}
		}
	}
	if(Pozar[PozarID][PozHP] > 200.0){
		KillTimer(Pozar[PozarID][PozTimer]);
		DestroyDynamic3DTextLabel(Pozar[PozarID][Text3D]);
		DestroyDynamicObject(Pozar[PozarID][Obiekt]);
		FoxForeach(i,Player){
  			if(IsPlayerInRangeOfPoint(i,10,Pozar[PozarID][Pozx],Pozar[PozarID][Pozy],Pozar[PozarID][Pozz])){
				format(string,sizeof(string),"{A5ACAA}Ogieñ po¿aru %i wypali³ siê..",PozarID);
				SendClientMessage(i,COLOR_WHITE,string);
				Pozar[PozarID][PozHP] = 0;
			}
		}
	} else if(Pozar[PozarID][PozHP] == 150){
		CreateExplosion(Pozar[PozarID][Pozx],Pozar[PozarID][Pozy],Pozar[PozarID][Pozz],11,Pozar[PozarID][Zasieg]);
		StworzPozar(Pozar[PozarID][Pozx]+float(random(9)-4),Pozar[PozarID][Pozy]+float(random(9)-4),Pozar[PozarID][Pozz],Pozar[PozarID][Zasieg]);
		StworzPozar(Pozar[PozarID][Pozx]+float(random(9)-4),Pozar[PozarID][Pozy]+float(random(9)-4),Pozar[PozarID][Pozz],Pozar[PozarID][Zasieg]);
	}
	if(Pozar[PozarID][PozHP] > 1.0){
		format(string,sizeof(string),"{DDDDDA}Po¿ar %i\n{DDDDDA}HP: {FF0000}%.2f",PozarID,Pozar[PozarID][PozHP]);
		UpdateDynamic3DTextLabelText(Pozar[PozarID][Text3D],COLOR_WHITE,string);
	} else {
		KillTimer(Pozar[PozarID][PozTimer]);
		DestroyDynamic3DTextLabel(Pozar[PozarID][Text3D]);
		DestroyDynamicObject(Pozar[PozarID][Obiekt]);
		Pozar[PozarID][PozHP] = 0;
		FoxForeach(i,Player){
			if(IsPlayerInRangeOfPoint(i,10,Pozar[PozarID][Pozx],Pozar[PozarID][Pozy],Pozar[PozarID][Pozz])){
				format(string,sizeof(string),"{A5ACAA}Po¿ar %i ugaszono..",PozarID);
				SendClientMessage(i,COLOR_WHITE,string);
				PieniadzeSP = PieniadzeSP + 1000;
			}
		}
	}
	return 1;
}

stock StworzPozar(Float:x, Float:y,Float:z,Float:range)
{
	for(new p = 1; p < 50; p++)
	{
		if(Pozar[p][PozHP] < 1)
		{
  			new string[255];
			Pozar[p][Obiekt] = CreateDynamicObject(18690,x,y,z-1.2,0,0,0);
			Pozar[p][PozTimer] = SetTimerEx("PozarTimer",200,true,"d",p);
			Pozar[p][Pozx] = x;
			Pozar[p][Pozy] = y;
			Pozar[p][Pozz] = z;
			Pozar[p][PozHP] = 100.0;
			Pozar[p][Zasieg] = range;
			Pozar[p][TrwaniePozaru] = 0;
			format(string,sizeof(string),"{DDDDDA}Po¿ar %i\n{DDDDDA}HP: {FF0000}%.2f",p,Pozar[p][PozHP]);
			Pozar[p][Text3D] = CreateDynamic3DTextLabel(string,COLOR_WHITE,x,y,z,4.5);
			return 1;
		}
	}
	return 1;
}

