}
public OnPlayerDeath(playerid, killerid, reason){
	if(killerid != INVALID_PLAYER_ID && !IsPlayerNPC(killerid)){
		if(gGracz[killerid][AdminLvl] < 7){
			if(gGracz[killerid][AdminDalBron] == false){
				switch(reason){
					case 2,8,25,26,27,28,30,32,33,35,36,37,38,39,55..254: {
						new buffer[256];
						format(buffer,sizeof buffer,"[HACK DETECT] [%s](%d)%s used FakeKill Reason: %d T:A",GetPlayerIP(playerid),playerid,GetPlayerNick(playerid),reason);
						print(buffer);
						WriteLog(buffer,LOG_FILE_HACK);
						MSGToAdmins(COLOR_RED,buffer,LEVEL_JUNIORADMIN);
						SendClientMessage(playerid,0xFF0000FF,"Zosta³eœ wyrzucony z serwera z powodu FakeKill.");
						BanEx(playerid,"[HD] FakeKill T:A");
						return 1;
					}
				}
			}
		}
	}
	if(killerid == INVALID_PLAYER_ID){
		switch(reason){
			case 0..34: {
				new buffer[200];
				format(buffer,sizeof buffer,"[HACK DETECT] [%s](%d)%s used FakeKill Reason: %d T:B",GetPlayerIP(playerid),playerid,GetPlayerNick(playerid),reason);
