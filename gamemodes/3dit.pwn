		}
	}
	if(medycy >= 1){
		SendClientMessage(playerid,COLOR_RED,"Jesteœ nieprzytomny! Czekaj a¿ ktoœ wezwie lekarza.");
		gGracz[playerid][Zemdlenie] = true;
		GetPlayerPos(playerid,gGracz[playerid][SmiercX],gGracz[playerid][SmiercY],gGracz[playerid][SmiercZ]);
