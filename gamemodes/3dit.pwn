public OdliczanieRSP(){
	if(odliczaniersp >= 1){
		if(odliczaniersp <= 10){
			new string[100];
			format(string,sizeof(string),"~b~Respawn za:~n~~r~%i",odliczaniersp);
			GameTextForAll(string,1000,4);
		}
		odliczaniersp--;
	} else {
		GameTextForAll("~r~[]~g~Respawn~r~[]",3000,4);
		new string[1000];
		if(AutomatycznyRespawnB == true){
			AutomatycznyRespawnB = false;
			SetTimer("OdblRespawn",60*1000,false);
			SendClientMessageToAll(COLOR_WHITE,"{C7C7C7}Wszystkie nieu¿ywane pojazdy zosta³y automatycznie zrespawnowane!");
		} else {
			format(string,sizeof(string),"{C7C7C7}Admin {00AAFF}%s {C7C7C7}zrespawnowa³ wszystkie nieu¿ywane pojazdy.",NickRSP);
			SendClientMessageToAll(COLOR_WHITE,string);
			respawnblock = 0;
		}
		for(new i = 0, j = GetVehiclePoolSize(); i <= j; i++){
			if(!IsValidVehicle(i)) continue;
			if(IsTrailerAttachedToVehicle(i)){
				new vehtrailer = GetVehicleTrailer(i);
				if(IsValidVehicle(vehtrailer)) TrailerLock[vehtrailer] = true;
			}
		}
		for(new i = 0, j = GetVehiclePoolSize(); i <= j; i++){
			if(!IsValidVehicle(i) || IsVehicleInUse(i) || BlokadaSpawnAuta[i]) continue;
			if(!TrailerLock[i]){
				SetVehicleToRespawn(i);
			}
			Paliwo[i] = GetVehicleMaxFuel(GetVehicleModel(i));
			TrailerLock[i] = false;
		}
		odliczaniersp = 0;
		KillTimer(timerrsp);
		respawnblock = 0;
		NickRSP = "SERVER";
	}
	return 1;
}

