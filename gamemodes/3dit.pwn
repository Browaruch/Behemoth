			pd++;
		}
	}
	if(m == 0 && (g == 0 || g == 2 || g == 4 || g == 6 || g == 8 || g == 10 || g == 12 || g == 14 || g == 16 || g == 18 || g == 20 || g == 22)){
		if(adm < 1 && pd < 3 && respawnblock == 0){
