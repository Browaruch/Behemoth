			}
		}
	}
	if(g == 0 && m == 0){
		printf("\n---------------------------------Nowy dzien %02i.%02i.%i\n----------------------------------\n",dzien,miesiac,year);
	}
	
	//Udswiez chopki
	for(new i = 0; i <= UzywanychChopkow; i++){
		new Float:x,Float:y,Float:z;
		GetActorPos(chopek[i],x,y,z);
		SetActorPos(chopek[i],x,y,z);
	}
	ChopkiUpdateAnim();
	return 1;
}

