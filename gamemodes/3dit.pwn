public OnPlayerUpdate(playerid){
	//if(playerid < 0 || playerid >= MAX_PLAYERS) return 1;
	/*
	if(IsPlayerInRectangle(playerid,-33.2106,1659.6918,423.8593,2122.6680))
	{
		if(!SkinyWojska(playerid) && gGracz[playerid][WiezienieWoj] == false){GameTextForPlayer(playerid,"~r~Teren wojska",1000,1);}
	}
	*/
	if(gGracz[playerid][acid_renia]){
		for(new i = 0; i < MAX_RENIA; i++){
			if(Acid_Renia_Ruch[i] && !FCNPC_IsMoving(Acid_Renia[i])){
				if(!Acid_Renia_Auto[i] && FCNPC_GetVehicleID(Acid_Renia[i]) == INVALID_VEHICLE_ID){
					if(5.0 <= GetDistanceBetweenPlayers(playerid,Acid_Renia[i]) < 100.0){
						FCNPC_GoToPlayerOnGroundCol(Acid_Renia[i],playerid,MOVE_TYPE_WALK,MOVE_SPEED_AUTO,true,1.0,2.0,true);
					}
				}
			}
		}
	}
	new Float:HPa,Float:xp,Float:yp,Float:zp;
 	GetPlayerPos(playerid,xp,yp,zp);
 	gGracz[playerid][NewX] = xp;
 	gGracz[playerid][NewY] = yp;
 	gGracz[playerid][NewY] = zp;
	if(IsPlayerInAnyVehicle(playerid) && IdVehicle[playerid] != INVALID_VEHICLE_ID){
		new Float:fx,Float:fy,Float:fz,Float:fr,Float:hpgracza;
		GetVehicleHealth(IdVehicle[playerid],HPa);
		GetPlayerHealth(playerid,hpgracza);
		GetVehiclePos(IdVehicle[playerid],fx,fy,fz);
		GetVehicleZAngle(IdVehicle[playerid],fr);
		if(HPa < 250.0 && (GetTickCount()-gGracz[playerid][pozar_auto] >= 5000)){
			gGracz[playerid][pozar_auto] = GetTickCount();
			SetVehicleHealth(IdVehicle[playerid],260.0);
			if(fx != 0 && fy != 0 && fz != 0){
				StworzPozar(fx,fy,fz,2);
			}
			SetVehiclePos(IdVehicle[playerid],fx,fy,fz);
			new engine, lights, alarm, bonnet, boot, doors, objective;
			GetVehicleParamsEx(IdVehicle[playerid],engine,lights,alarm,doors,bonnet,boot,objective);
			SetVehicleParamsEx(IdVehicle[playerid],0,lights,alarm,doors,1,boot,objective);
			new Panels, Doors, Lights, Tires;
			GetVehicleDamageStatus(IdVehicle[playerid], Panels, Doors, Lights, Tires);
			UpdateVehicleDamageStatus(IdVehicle[playerid], Panels, Doors, Lights, 10);
		}
	}
	if(IsPlayerInRangeOfPoint(playerid,50,51.3340,1529.4572,15.2485) || IsPlayerInRangeOfPoint(playerid,50,1402.3279,-4688.9917,6.8709))
	{
		if(Disco == true)
		{
			if(gGracz[playerid][SluchaRadia] == false)
			{
				StopAudioStreamForPlayer(playerid);
				PlayAudioStreamForPlayer(playerid,LinkRadia,51.3340,1529.4572,15.2485,50.0,1);
				PlayAudioStreamForPlayer(playerid,LinkRadia,1402.3279,-4688.9917,6.8709,50.0,1);
				gGracz[playerid][SluchaRadia] = true;
			}
		}
	}
	
	new iCurWeap = GetPlayerWeapon(playerid);
	if(iCurWeap != gGracz[playerid][last_weapon]){
		OnPlayerChangeWeapon(playerid,gGracz[playerid][last_weapon],iCurWeap);
		gGracz[playerid][last_weapon] = iCurWeap;
	}
	return 1;
}

